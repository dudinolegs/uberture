<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Uberture</title>
</head>
<body>
    
    <%= require('../templates/header_default.html').default %>

    <%= require('../templates/header_fixed.html').default %>

    <section class="page-title">
        <div class="container">
            <div class="breadcrumbs">
                <div class="breadcrumbs__wrapper">
                    <a class="breadcrumbs__item" href="#">Главная</a>
                    <a class="breadcrumbs__item" href="#">Личный кабинет</a>
                    <a class="breadcrumbs__item" href="#">Документация</a>
                    <span class="breadcrumbs__item">Брендбук</span>
                </div>
            </div>
            <h1>Брендбук</h1>
        </div>
    </section>
    
    <div class="section">
        <div class="container">
            <div class="personal-wrapper">
                <aside class="personal-sidebar">
                    <div class="personal-sidebar-search">
                        <%= require('../templates/form_search.html').default %>
                    </div>

                    <%= require('../templates/personal_sidebar.html').default %>

                    <div class="personal-sidebar-promo">
                        <%= require('../templates/personal_promo.html').default %>
                    </div>
                </aside>

                <main class="personal-main">
                    <div class="personal-main-search">
                        <%= require('../templates/form_search.html').default %>
                    </div>
                    
                    <div class="personal-brandbook-list">
                        <%= require('../templates/personal_brandbook_page.html').default %>
                    </div>

                    <div class="personal-main-promo">
                        <%= require('../templates/personal_promo.html').default %>
                    </div>
                </main>
            </div>
        </div>
    </div>

    <%= require('../templates/social.html').default %>
    
    <%= require('../templates/footer.html').default %>

</body>
</html>